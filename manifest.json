{
    "name": "Auto Reload page",
    "version": "1.0",
    "description": "Keep reloading page automatically!",
    "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
      },
    "manifest_version": 2
  }